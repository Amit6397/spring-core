<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean class="com.sunglowsys.autowiringUsingXml.Address" name="address">
         <property name="street" value="Nagla man singh"/>
          <property name="city" value="Etah"/>

    </bean>
    <!-- autowiring use byName and byType and constructor     !-->
       <bean class="com.sunglowsys.autowiringUsingXml.Emp" name="e" autowire="constructor"/>


</beans>